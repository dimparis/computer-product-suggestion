        <script>
            // check all in table
            function checkboxAllTable(i) {
                if ($("#checkboxAllTable_" + i).find("input:checkbox")[0].checked == true) {
                    $("#table_" + i).find("tbody").find("input:checkbox").attr('checked', $(this).is('') ? 'checked' : '');
                    $("#table_" + i).find("tbody").find("tr").attr("class", "checked");
                }
                else {
                    $("#table_" + i).find("tbody").find("input:checkbox").removeAttr('checked');
                    $("#table_" + i).find("tbody").find("tr").removeClass('checked');
                }
            }

            // Tab sản phẩm trùng
            function tabsanphamtrung() {  
                var val = "nothing";                                                                                          
                var postData = { valuesgop: val };
                $.ajax({
                    type: "POST",
                    url: "/Admin/TrainingProduct/gopdatabase",
                    data: postData,
                    success: function(data){
                        $('#black-icon').html(data);
                    },

                });     
            }


            //xóa sản phẩm đúng.
            function xoasanphamdung(stt){
                if (confirm("Bạn có chắc chắn muôn xóa sản phẩm này?")) {
                    var postData = { sttproduct: stt };
                    $.ajax({
                        type: "POST",
                        url: "/Admin/ImportExcelLap/deleteproduct",
                        data: postData,
                        success: function(data){
                            $('#CorrectID').html(data);
                            tabsanphamdung();
                        },
                    });
                } else {
                                               
                }
                                            
            }



                                                
            // Tách 1 table
            function tach1table(i) {  
                var val = "";
                var count =0;
                $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
                    val +='*'+ $(this).val();
                    count ++;
                });   
                
                if(count==1){
                    $.jGrowl("Để tách bảng vui lòng chọn ít nhất 2 sản phẩm", {header: "Nhắc nhở!", position: "bottom-right"});
                }
                // thêm
                $("#table_" + i).find("tbody").find("input:checkbox").each(function(i){                                               
                    val +='@@'+ $(this).val();
                });      
                var postData = { valuest: val };
                $.ajax({
                    type: "POST",
                    url: "/Admin/TrainingProduct/getTach",
                    data: postData,
                    success: function(data){
                        $('#color-icon').html(data);
                        // load lại tab product trùng database
                        tabsanphamtrung();
                    },

                });     
            }
            //Gộp 1 talbe
            function gop1table(i) {  
                var val = "";
                var check =[];
                var radio ="";
                var count = 0;
                $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
                    val +='@@'+ $(this).val();
                    check[i]= $(this).val();
                    count ++;
                });
                if(count>=2){
                   
                    $("#table_" + i).find("tbody").find("input:radio:checked").each(function(i){  
                        val += '@@'+ $(this).val();
                        radio = $(this).val();
                    });
                    for(var x =0; x <check.length; x++)
                    {
                        if(check[x] == radio){
                            var postData = { valuest: val };
                            $.ajax({
                                type: "POST",
                                url: "/Admin/TrainingProduct/getGop",
                                data: postData,
                                success: function(data){
                                    $('#color-icon').html(data);
                                    // load lại tab product trùng database
                                    tabsanphamtrung();
                                },                                              
                            });  
                            break;
                        }else{
                            $.jGrowl("Vui lòng chọn tên chính để gộp sản phẩm", {header: "Nhắc nhở!", position: "bottom-right"});
                            break;
                        }
                    }
                   
                }else{
                    $.jGrowl("Để gộp vui lòng chọn ít nhất 2 sản phẩm", {header: "Nhắc nhở!", position: "bottom-right"});
                }            
                //   location.reload();
            }
            // tách table khi so trùng database
            function tachdatabase(i)
            {
                var val = "";
                $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
                    val +='@@'+ $(this).val();
                });
                var postData = { valuestach: val };
                $.ajax({
                    type: "POST",
                    url: "/Admin/TrainingProduct/tachdatabase",
                    data: postData,
                    success: function(data){
                        $('#black-icon').html(data);
                      //  tabsanphamdung();                                               
                    },                                             
                });  

            }
            // gộp table khi so trùng database
            function gopdatabase(i)
            {
                var val = "";
                $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
                    val +='@@'+ $(this).val();
                });
                var postData = { valuesgop: val };
                $.ajax({
                    type: "POST",
                    url: "/Admin/TrainingProduct/gopdatabase",
                    data: postData,
                    success: function(data){
                        $('#black-icon').html(data);
                     //   tabsanphamdung();                                               
                    },

                                                
                });  

            }
        </script>
