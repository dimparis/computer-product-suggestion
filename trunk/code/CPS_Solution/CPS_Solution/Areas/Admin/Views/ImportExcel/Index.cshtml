                    type: "POST",
                    url: "/Admin/ImportExcel/deleteproduct",
                    data: postData,
                    success: function(data){
                        $('#CorrectID').html(data);
                    },

                });
                                                  
            }


            //xóa sản phẩm đúng.
            function xoasanphamdung(stt){

                if (confirm("Bạn có chắc chắn muôn xóa sản phẩm này?")) {
                    var postData = { sttproduct: stt };
                    $.ajax({
                        type: "POST",
                        url: "/Admin/ImportExcel/deleteproduct",
