                    $.ajax({
                        type: "POST",
                        url: "/Admin/ImportExcel/fixedErrorProduct",
                        data: postData,
                        success: function(data){
                            $('#white-icon').html(data);
                            tabsanphamdung();
                            tabsanphamtrung();
                        },

                    });
                } else {
                                               
                }
            }



