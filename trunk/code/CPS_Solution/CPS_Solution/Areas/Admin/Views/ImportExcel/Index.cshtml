                $.ajax({
                    type: "POST",
                    url: "/Admin/ImportExcel/gopdatabase",
                    data: postData,
                    success: function(data){
                        $('#black-icon').html(data);
                        tabsanphamdung();                                               
                    },

                                                
                });  

            }
