                $('#fileupload').bind('change', function() {
                    // alert('This file size is: ' + this.files[0].size/1024/1024 + "MB");
                    //alert('This file name is: ' + this.files[0].name); 
                    fsize = this.files[0].size/1024/1024;
                    fname =  this.files[0].name;
                 
                });

                function checkuploadfile(){  
                    var check1 = fname.indexOf("xlsx");
                    var check2 = fname.indexOf("xls");
                    if(check1 >0||check2>0)
                    {
                        if(fsize>2){
                            $.jGrowl("File không được lớn hơn 2Mb", {header: "Nhắc nhở!", position: "bottom-right"});
                        }else{
                            document.getElementById("da-ex-vali").submit();
                        }
                    }else{
                        $.jGrowl("Hệ thống chỉ hỗ trợ file excel vui lòng thử lại!", {header: "Nhắc nhở!", position: "bottom-right"});
                    }
                }

                //Hiện popup để sửa sản phẩm khi tách.
                function showPopupEditTach(i){
                    $("#Edit-"+i+"-Popup-div").dialog({
                        autoOpen: false, 
                        title: "Bạn có muốn sửa trước khi lưu!", 
                        width: "600", 
                        modal: true             
                        //buttons: [{
                        //    text: "Đóng", 
                        //    click: function() {
                        //        $( this ).dialog( "close" );
                        //    }}]
                    });

                    $("#Edit-"+i+"-Popup-div").dialog("option", {modal: true}).dialog("open");
                    event.preventDefault();
		
                }
            
                // show Info số lượng trong danh sách đúng trùng lỗi
                function showInfo()
                {
                    string = "ok";
                    var postData = {showinfo: string };
                    $.ajax({
                        type: "POST",
                        url: "/Admin/ImportExcel/showInfo1",
                        data: postData,
                        success: function(data){
                            $('#showInfo').html(data);
                        },

                    });
                }
            // Lưu sản phẩm đúng và ghi log file
                function saveCorrectProduct(x) { 
                var val = ""; 
                var err =  document.getElementById("IdErrorLog").checked;
                var dup =  document.getElementById("IdDuplicateLog").checked;
                if(dup==true){
                    val = "dup";
                }else{
                    val = "no";
                }
                if(err==true){
                    val += "@@err";
                }else{
                    val += "@@no";
                }
                var postData = { checkval: val };
                if(x >0){

                    $.ajax({
                        type: "POST",
                        url: "/Admin/ImportExcel/saveAllProduct",
                        data: postData,
                        success: function(data){
                            location.reload();
                        },

                    }); 
                }else{
                    $("#da-ex-dialog-div").dialog("option", {modal: false}).dialog("close");       
                    $.jGrowl("Hiện chưa có sản phẩm nào để lưu. Vui lòng thử lại!", {header: "Nhắc nhở!", position: "bottom-right"});
                }
            }
            // check all in table
            function checkboxAllTable(i) {
                if ($("#checkboxAllTable_" + i).find("input:checkbox")[0].checked == true) {
                    $("#table_" + i).find("tbody").find("input:checkbox").attr('checked', $(this).is('') ? 'checked' : '');
                    $("#table_" + i).find("tbody").find("tr").attr("class", "checked");
                }
                else {
                    $("#table_" + i).find("tbody").find("input:checkbox").removeAttr('checked');
                    $("#table_" + i).find("tbody").find("tr").removeClass('checked');
                }
            }


            // lưu sản phẩm lỗi
            function luusanphamloi(stt){
                var trongso =  document.getElementById("idTrongSo_"+stt).value;
                var loaithanhphan =  document.getElementById("idLoai_"+stt).value;
                var tenthanhphan =  document.getElementById("idTen_"+stt).value;
                var string = stt +'@@'+tenthanhphan+'@@'+loaithanhphan+'@@'+ trongso;
                if (confirm("Bạn có chắc chắn muốn lưu sản phẩm này?")) {
