                <!--tab1------------------------------------------------------------------------------------->
                <div id="luuhuyID" class="da-panel-toolbar top">
                    <ul>
                        <form action="~/Admin/ImportExcel/saveAllProduct" method="post" enctype="multipart/form-data">
                            <li>
                                <a id="da-ex-dialog-modal">
                                    <img src="~/Areas/Admin/Dandelion/images/icons/color/accept.png" alt="">Lưu</a></li>
                            <!----------------Popup-----------
                                 <button id="da-ex-dialog-modal" style="display: none;" class="da-button blue">Modal UI Dialog</button>------->
