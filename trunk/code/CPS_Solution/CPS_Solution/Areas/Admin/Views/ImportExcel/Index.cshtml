                                    <input style="display:none;" class="uniform" id="IdErrorLog" style="zoom:1.8;" type="checkbox" name="sports[]"> <label style="display:none;"> <img src="~/Areas/Admin/Dandelion/images/message-error.png" alt="">Danh sách sản phẩm lỗi</label>
                                    <button type="button" onclick="saveCorrectProduct(@countlist)" class="da-button blue" style=" float: right;" role="button" aria-disabled="false"><span class="ui-button-text">Hoàn thành</span></button>
