                    </ul>
                </div>

            @if (errorLine != null)
            {
                if (sodongloi > 5)
                { 
                    <label style="color: red; font-weight: bold; font-size: larger">File excel có quá nhiều lỗi vui lòng chỉnh sửa lại!</label> <br />
