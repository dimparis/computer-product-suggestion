    <script type="text/javascript">
        function createMasonryElement() {
            var image1 = '<img src="images/portfolio-4columns-image1.png" alt="portfolio-image" />';
            var image2 = '<img src="images/portfolio-4columns-image2.png" alt="portfolio-image" />';
            var image3 = '<img src="images/portfolio-4columns-image3.png" alt="portfolio-image" />';
            var image4 = '<img src="images/portfolio-4columns-image4.png" alt="portfolio-image" />';
            var image5 = '<img src="images/portfolio-4columns-image5.png" alt="portfolio-image" />';
            var image6 = '<img src="images/portfolio-4columns-image6.png" alt="portfolio-image" />';
            var image7 = '<img src="images/portfolio-4columns-image7.png" alt="portfolio-image" />';

            var elements = '';
            var element = '';
            var image = '';

            for (var i = 0; i < 2; i++) {
                var randomNumber = Math.random();

                if (randomNumber < 0.15)
                    image = image1;
                else if (randomNumber < 0.30)
                    image = image2;
                else if (randomNumber < 0.45)
                    image = image3;
                else if (randomNumber < 0.60)
                    image = image4;
                else if (randomNumber < 0.75)
                    image = image5;
                else if (randomNumber < 0.90)
                    image = image6;
                else
                    image = image7;

                element =
                        '<div class="isotope-item post span6">' +
                                '<div class="row">' +
                                '<div class="span3">' +
                                '<div class="post-media">' +
                                '<iframe src="http://player.vimeo.com/video/25451551" frameborder="0" webkitAllowFullScreen mozallowfullscreen allowFullScreen></iframe>' +
                                '</div>' +
                                '</div>' +

                                '<div class="span3">' +
                                '<div class="post-content">' +
                                '<div class="post-title">' +
                                '<h3><i class="icon-film post-title-icon"></i>Short Movie</h3>' +
                                '</div>' +

                                '<div class="post-info clearfix">' +
                                '<div class="post-date"><i class="icon-calendar"></i> <p>Nov 10, 2012</p></div>' +
                                '<div class="post-category"><i class="icon-pencil"></i> <a href="#">Design</a></div>' +
                                '<div class="post-comments"><i class="icon-comments"></i> <a href="#">(3)</a></div>' +
                                '</div>' +

                                '<div style="clear: both"></div>' +

                                '<div class="post-excerpt">' +
                                '<p>' +
                                'Donec magna nunc, vestibulum' +
                                'at erat ac, mollis varius nunc. Maecenas nec hendrerit nunc. Integer interdum neque lorem, eget mattis dui vehicula at.' +
                                '</p>' +
                                '</div>' +

                                '<div class="stylish-button"><a href="#">read more</a></div>' +
                                '</div>' +
                                '</div>' +
                                '</div>' +
                                '</div>';

                elements += element;
            }


            var elementsString = jQuery(elements);

            return elementsString;
        }
    </script>
