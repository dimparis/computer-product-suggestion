        loiCPU = errorLineLap[4].ToString().Split(',');
        // lỗi HDD
        loiHDD = errorLineLap[5].ToString().Split(',');
        // lỗi VGA
        loiVGA = errorLineLap[6].ToString().Split(',');
        // lỗi Ram
        loiRam = errorLineLap[7].ToString().Split(',');
    }
}

