            //    }}]
        });

        $("#Edit-"+i+"-Popup-div").dialog("option", {modal: true}).dialog("open");
        event.preventDefault();
		
    }
    // change image on popup when click
    function ChangeImagePopup(i){
        document.getElementById("idViewImage"+i).src=document.getElementById("idImagelinkCorect"+i).value;
