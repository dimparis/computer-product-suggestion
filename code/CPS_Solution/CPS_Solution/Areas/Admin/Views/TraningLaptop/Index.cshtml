    function checkboxAllTable(i) {
        if ($("#checkboxAllTable_" + i).find("input:checkbox")[0].checked == true) {
            $("#table_" + i).find("tbody").find("input:checkbox").attr('checked', $(this).is('') ? 'checked' : '');
            $("#table_" + i).find("tbody").find("tr").attr("class", "checked");
        }
        else {
            $("#table_" + i).find("tbody").find("input:checkbox").removeAttr('checked');
            $("#table_" + i).find("tbody").find("tr").removeClass('checked');
        }
    }


    // lưu sản phẩm lỗi
    function luusanphamloi(stt){
        var Name =  document.getElementById("idName_"+stt).value;
        var ImageLink =  document.getElementById("idImageLink_"+stt).value;
        var CPU =  document.getElementById("idCPU_"+stt).value;
        var VGA =  document.getElementById("idVGA_"+stt).value;
        var HDD =  document.getElementById("idHDD_"+stt).value;
        var Display =  document.getElementById("idDisplay_"+stt).value;
        var Ram =  document.getElementById("idRam_"+stt).value;
        var Price = document.getElementById("idPrice_"+stt).value;
        var URL = document.getElementById("idURL_"+stt).value;
        var string = stt +'@@'+Name+'@@'+ImageLink+'@@'+ CPU+'@@'+ VGA+'@@'+ HDD+'@@'+ Display+'@@'+ Ram+'@@'+ Price+'@@'+ URL;
        if (confirm("Bạn có chắc chắn muốn lưu sản phẩm này?")) {
            var postData = { stringpro: string };
            $.ajax({
                type: "POST",
                url: "/Admin/TraningLaptop/fixedErrorProduct",
                data: postData,
                success: function(data){
                    $('#white-icon').html(data);
                    tabsanphamdung();
                    tabsanphamtrung();
                },

            });
        } else {
                                               
        }
    }
    // Sửa sản phẩm đúng
    function EditCorrectPro(stt){              
        // kiểm tra image
        img = new Image();
        var checkImg =0;   
           
        // nếu image load được thì check tăng lên 1
        img.onload = function(){
            checkImg ++;
        };
        var Name =  document.getElementById("idNameCorect"+stt).value;
        var ImageLink =  document.getElementById("idImagelinkCorect"+stt).value;
        img.src = ImageLink;
        var CPU =  document.getElementById("idCPUCorect"+stt).value;
        var VGA =  document.getElementById("idVGACorect"+stt).value;
        var HDD =  document.getElementById("idHDDCorect"+stt).value;
        var Display =  document.getElementById("idDisplayCorect"+stt).value;
        var Ram =  document.getElementById("idRamCorect"+stt).value;
        var Price = document.getElementById("idPriceCorect"+stt).value;
        var Url = document.getElementById("idUrlCorect"+stt).value;
        var string = stt +'@@'+Name+'@@'+ImageLink+'@@'+ CPU+'@@'+ VGA+'@@'+ HDD+'@@'+ Display+'@@'+ Ram+'@@'+ Price+'@@'+ Url;
        var count =0;                            
        // kiểm tra tên
        if(Name.length<5||Name.length>100)
        {
            document.getElementById("idNameCorect"+stt).style.borderColor='red';
            count++;
        }else {
            document.getElementById("idNameCorect"+stt).style.borderColor="";
        }           
        //kiểm tra cpu
        if(CPU.length<5||CPU.length>100)
        {
            document.getElementById("idCPUCorect"+stt).style.borderColor='red';
            count++;
        }else{
            document.getElementById("idCPUCorect"+stt).style.borderColor="";
        }
        // kiểm tra VGA
        if(VGA.length<5||VGA.length>100)
        { 
            document.getElementById("idVGACorect"+stt).style.borderColor='red';
            count++;
        }else{
            document.getElementById("idVGACorect"+stt).style.borderColor="";
        }
        // kiểm tra HDD
        if(HDD.length<1||HDD.length>100)
        {
            document.getElementById("idHDDCorect"+stt).style.borderColor='red';
            count++;
        }else{
            document.getElementById("idHDDCorect"+stt).style.borderColor="";
        }
        // kiem tra Display
        if(Display.length<5||Display.length>100)
        {
            document.getElementById("idDisplayCorect"+stt).style.borderColor='red';
            count++;
        }else{
            document.getElementById("idDisplayCorect"+stt).style.borderColor="";
        }
        // kiểm tra Ram
        if(Ram.length<1||Ram.length>100)
        {
            document.getElementById("idRamCorect"+stt).style.borderColor='red';
            count++;
        }else{
            document.getElementById("idRamCorect"+stt).style.borderColor="";
        }
        // kiểm tra giá
        myFunction(Price);
        function myFunction(Price) {
            val = parseFloat(Price);
            if(isNaN(val)){
                document.getElementById("idPriceCorect"+stt).style.borderColor='red';
                count++;
            }else{
                document.getElementById("idPriceCorect"+stt).style.borderColor="";
            }
        }
        //kiểm tra URL             
        if(checkUrl(Url) ==false)
        {
            document.getElementById("idUrlCorect"+stt).style.borderColor='red';
            count++;
        }else{
            document.getElementById("idUrlCorect"+stt).style.borderColor="";
        }
        //            
        function checkUrl(url) {
            var request = false;
            if (window.XMLHttpRequest) {
                request = new XMLHttpRequest;
            } else if (window.ActiveXObject) {
                request = new ActiveXObject("Microsoft.XMLHttp");
            }
            if (request) {
                request.open("GET", url);
                if (request.status == 200)
                { return true; }
                if( request.status == 404) 
                { return false; }
            }
                   
                

               
        }
                   
         
        // kiểm tra image
        setTimeout(function () {
            if(checkImg==0){                   
                document.getElementById("idImagelinkCorect"+stt).style.borderColor ='red';
                count++;
            }else{
                document.getElementById("idImagelinkCorect"+stt).style.borderColor="";
            }
            if(count==0)
            {
                var postData = {stringprocorrect: string };
                $.ajax({
                    type: "POST",
                    url: "/Admin/TraningLaptop/EditCorrectPro",
                    data: postData,
                    success: function(data){
                        // $('#white-icon').html(data);
                        $("#Edit-"+stt+"-Popup-div").dialog( "close");
                        tabsanphamdung();
                        tabsanphamtrung();
                        showInfo();
                    },

                });
            }
        }, 1000);         
        // nếu không phát hiện lỗi thì gửi về controller để xử lý                       
    }   
            

