    //xóa sản phẩm đúng.
    function xoasanphamdung(stt){
        if (confirm("Bạn có chắc chắn muôn xóa sản phẩm này?")) {
            var postData = { sttproduct: stt };
            $.ajax({
                type: "POST",
                url: "/Admin/TraningLaptop/deleteproduct",
                data: postData,
                success: function(data){
                    $('#CorrectID').html(data);
                    tabsanphamdung();
                },
            });
        } else {
                                               
        }
                                            
    }



                                                
    // Tách 1 table
    function tach1table(i) {  
        var val = "";
        var count =0;
        $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
            val +='*'+ $(this).val();
            count ++;
        });   
                
        if(count==1){
            $.jGrowl("Để tách bảng vui lòng chọn ít nhất 2 sản phẩm", {header: "Nhắc nhở!", position: "bottom-right"});
        }
        // thêm
        $("#table_" + i).find("tbody").find("input:checkbox").each(function(i){                                               
            val +='@@'+ $(this).val();
        });      
        var postData = { valuest: val };
        $.ajax({
            type: "POST",
            url: "/Admin/TraningLaptop/getTach",
            data: postData,
            success: function(data){
                $('#black-icon').html(data);
                tabsanphamdung();
            },

        });     
    }
    //Gộp 1 talbe
    function gop1table(i) {  
        var val = "";
        var check =[];
        var radio ="";
        var count = 0;
        $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
            val +='@@'+ $(this).val();
            check[i]= $(this).val();
            count ++;
        });
        if(count>=2){
                   
            $("#table_" + i).find("tbody").find("input:radio:checked").each(function(i){  
                val += '@@'+ $(this).val();
                radio = $(this).val();
            });
            for(var x =0; x <check.length; x++)
            {
                if(check[x] == radio){
                    var postData = { valuest: val };
                    $.ajax({
                        type: "POST",
                        url: "/Admin/TraningLaptop/getGop",
                        data: postData,
                        success: function(data){
                            $('#black-icon').html(data);
                            tabsanphamdung();                                               
                        },                                              
                    });  
                    break;
                }else{
                    $.jGrowl("Vui lòng chọn tên chính để gộp sản phẩm", {header: "Nhắc nhở!", position: "bottom-right"});
                    break;
                }
            }
                   
        }else{
            $.jGrowl("Để gộp vui lòng chọn ít nhất 2 sản phẩm", {header: "Nhắc nhở!", position: "bottom-right"});
        }            
        //   location.reload();
    }
