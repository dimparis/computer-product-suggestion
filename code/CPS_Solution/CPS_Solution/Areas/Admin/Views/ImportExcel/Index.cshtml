                var string="nothing";
                var postData = { sttproduct: string };
                $.ajax({
                    type: "POST",
                    url: "/Admin/ImportExcel/deleteproduct",
                    data: postData,
                    success: function(data){
                        $('#color-icon').html(data);
                    },

                });
                                                  
            }


            //xóa sản phẩm đúng.
            function xoasanphamdung(stt){

                if (confirm("Bạn có chắc chắn muôn xóa sản phẩm này?")) {
                    var postData = { sttproduct: stt };
                    $.ajax({
                        type: "POST",
                        url: "/Admin/ImportExcel/deleteproduct",
                        data: postData,
                        success: function(data){
                            $('#CorrectID').html(data);
                            tabsanphamdung();
                        },

                    });
                } else {
                                               
                }
                                            
            }



                                                
            // Tách 1 table
            function tach1table(i) {  
                var val = "";
                $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
                    val +='@@'+ $(this).val();
                });                                          
                var postData = { valuest: val };
                $.ajax({
                    type: "POST",
                    url: "/Admin/ImportExcel/getTach",
                    data: postData,
                    success: function(data){
                        //   $('#tachid').html(data);
                        tabsanphamtrung()
                        tabsanphamdung();
                    },

                });     
            }
            //Gộp 1 talbe
            function gop1table(i) {  
                var val = "";
                $("#table_" + i).find("tbody").find("input:checkbox:checked").each(function(i){                                               
                    val +='@@'+ $(this).val();
                });
                $("#table_" + i).find("tbody").find("input:radio:checked").each(function(i){  
                    val += '@@'+ $(this).val();
                }); 
                var postData = { valuest: val };
                $.ajax({
                    type: "POST",
                    url: "/Admin/ImportExcel/getGop",
                    data: postData,
                    success: function(data){
                        tabsanphamtrung()
                        tabsanphamdung();                                               
                    },

                                                
                });  
